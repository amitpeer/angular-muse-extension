{
  "manifest_version": 2,
  "name": "Extension",
  "description": "Angular CLI Chrome app. ",
  "version": "1.0",
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "assets/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "assets/jquery-3.2.1.min.js",
        "assets/painter.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
